#include <iostream>
using namespace std;

int main()
{
    int x;
    cout << "Please type in a number between 1-10. \nIf you type in the wrong number than your mother is gay." << endl;
    cin >> x;



if(8>x)
{
    cout << "Too low! Your mother is gay!" << endl;
}

if(8<x)
{
    cout << "Wrong! Too high! Your mom is gay!" << endl;
}

if(8==x)
{
    cout<<"Correct! Your mother is half gay, or bisexual."<<endl;
}
}
